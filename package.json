{
  "name": "multi-agent-browser-testing",
  "version": "1.0.0",
  "description": "AI-powered QA testing system with parallel Playwright agents and Supabase",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/startWithStream.js",
    "start:no-stream": "node src/index.js",
    "start:video": "ENABLE_VIDEO_RECORDING=true ENABLE_LIVE_STREAM=true node src/index.js",
    "start:with-stream": "node src/startWithStream.js",
    "agent": "node src/agent/worker.js",
    "agent:video": "ENABLE_VIDEO_RECORDING=true ENABLE_LIVE_STREAM=true node src/agent/worker.js",
    "spawn": "node src/orchestration/spawner.js",
    "monitor": "node src/orchestration/monitor.js",
    "setup:db": "node src/setup/database.js",
    "load:samples": "node examples/loadSampleTests.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:backend": "./test-backend.sh",
    "test:backend:parallel": "npm run test:backend & npm run test:backend & npm run test:backend & wait",
    "test:single": "HEADLESS=false npm run agent",
    "test:watch": "tail -f logs/combined.log",
    "test:agent": "TEST_MODE=true HEADLESS=true npm run agent",
    "test:spawn": "TEST_MODE=true npm run spawn",
    "stream:server": "node src/server/videoStream.js"
  },
  "keywords": [
    "playwright",
    "supabase",
    "testing",
    "automation",
    "qa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "p-queue": "^8.0.1",
    "playwright": "^1.40.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
